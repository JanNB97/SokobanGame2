<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox onKeyTyped="#handleTypedOnKey" prefHeight="964.0" prefWidth="1153.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.MainController">
  <children>
      <HBox prefHeight="47.0" prefWidth="1195.0">
         <children>
            <Button mnemonicParsing="false" onMouseClicked="#handleClickedOnRestart" text="Restart">
               <HBox.margin>
                  <Insets left="10.0" right="10.0" top="10.0" />
               </HBox.margin>
            </Button>
            <Button mnemonicParsing="false" onMouseClicked="#handleClickedOnUndo" text="Undo">
               <HBox.margin>
                  <Insets top="10.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
      <VBox fx:id="mainVBox" prefHeight="200.0" prefWidth="100.0" VBox.vgrow="ALWAYS" />
  </children>
</VBox>
